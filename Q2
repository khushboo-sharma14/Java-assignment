class Main{    
 public static void main(String args[]){    
  int i,f=0;      
  int num=3;      
  if(num==0||num==1)
  {  
   System.out.println("Not a prime number !");      
  }
  else
  {  
   for(i=2;i<=num/2;i++)
   {      
    if(num%i==0)
    {      
     System.out.println("Not a prime number !");      
     f=1;      
     break;      
    }      
   }      
   if(f==0)  
   { System.out.println("It is a prime number"); }  
  }
}    
} 
